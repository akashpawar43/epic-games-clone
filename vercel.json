{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.js"
        },
        {
            "src": "/games/(.*)",
            "dest": "/views/partials/games.hbs"
        }
    ]
}